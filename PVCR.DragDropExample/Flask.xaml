<UserControl x:Class="PVCR.DragDropExample.Flask"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PVCR.DragDropExample"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" AllowDrop="True">

    <UserControl.Resources>
        <ImageBrush ImageSource="images/Flask-icon.png" x:Key="whiteStoneBrush"/>
    </UserControl.Resources>
    <Grid>
        <Ellipse x:Name="flaskUI"  Fill="{StaticResource whiteStoneBrush}" Width="50" Height="50"/>
    </Grid>
</UserControl>
